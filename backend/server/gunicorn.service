[Unit]
Description=gunicorn daemon for Django project
After=network.target

[Service]
User=ec2-user
Group=www-data
WorkingDirectory=/home/ec2-user/Coverly/server 
ExecStart=/home/ec2-user/.local/bin/gunicorn --workers 3 --bind unix:/home/ec2-user/Coverly/server.sock server.wsgi:application

[Install]
WantedBy=multi-user.target
